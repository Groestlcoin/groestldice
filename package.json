{
  "name": "bfs-dice",
  "version": "0.1.0",
  "description": "Bitcoin toolbox for Hodlers and Bitcoiners",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "browserify:main-page": "browserify app/js/main.js -o app/js/dist/main_bundle.js",
    "browserify:step-one": "browserify app/js/step-one.js -o app/js/dist/step-one_bundle.js",
    "browserify:step-two": "browserify app/js/step-two.js -o app/js/dist/step-two_bundle.js",
    "browserify:step-three": "browserify app/js/step-three.js -o app/js/dist/step-three_bundle.js",
    "browserify:step-four": "browserify app/js/step-four.js -o app/js/dist/step-four_bundle.js",
    "browserify:util-seed_gen": "browserify app/js/util-seed_gen.js --exclude=node_modules/bip39/src/wordlists/japanese.json --exclude=node_modules/bip39/src/wordlists/chinese_simplified.json --exclude=node_modules/bip39/src/wordlists/chinese_traditional.json --exclude=node_modules/bip39/src/wordlists/french.json --exclude=node_modules/bip39/src/wordlists/italian.json --exclude=node_modules/bip39/src/wordlists/korean.json --exclude=node_modules/bip39/src/wordlists/spanish.json -o app/js/dist/util-seed_gen_bundle.js",
    "browserify:contact-page": "browserify app/js/contact-page.js -o app/js/dist/contact-page_bundle.js",
    "browserify:localizer": "browserify app/js/localizer.js -o app/js/dist/localizer_bundle.js",
    "generate-content:step-one_fa-ir": "./node_modules/marked/bin/marked ./translations/fa-ir/step-one.md -o ./translations/fa-ir/dist/step-one-content.html",
    "generate-content:step-two_fa-ir": "./node_modules/marked/bin/marked ./translations/fa-ir/step-two.md -o ./translations/fa-ir/dist/step-two-content.html",
    "generate-content:step-three_fa-ir": "./node_modules/marked/bin/marked ./translations/fa-ir/step-three.md -o ./translations/fa-ir/dist/step-three-content.html",
    "generate-content:step-four_fa-ir": "./node_modules/marked/bin/marked ./translations/fa-ir/step-four.md -o ./translations/fa-ir/dist/step-four-content.html",
    "generate-content:contact-page_fa-ir": "./node_modules/marked/bin/marked ./translations/fa-ir/contact-page.md -o ./translations/fa-ir/dist/contact-page-content.html",
    "generate-content:step-one_en-us": "./node_modules/marked/bin/marked ./translations/en-us/step-one.md -o ./translations/en-us/dist/step-one-content.html",
    "generate-content:step-two_en-us": "./node_modules/marked/bin/marked ./translations/en-us/step-two.md -o ./translations/en-us/dist/step-two-content.html",
    "generate-content:step-three_en-us": "./node_modules/marked/bin/marked ./translations/en-us/step-three.md -o ./translations/en-us/dist/step-three-content.html",
    "generate-content:step-four_en-us": "./node_modules/marked/bin/marked ./translations/en-us/step-four.md -o ./translations/en-us/dist/step-four-content.html",
    "generate-content:contact-page_en-us": "./node_modules/marked/bin/marked ./translations/en-us/contact-page.md -o ./translations/en-us/dist/contact-page-content.html",
    "package:linux": "electron-packager . bfs-dice_0.1.0_RC2 --icon=assets/images/bfs-app-icon-v5-512.png --out=packages --platform=linux --arch=x64 --overwrite --prune=true --ignore=dev --ignore=.gitignore --ignore=.git --ignore=gpg-pub-key --ignore=tests --ignore=releases",
    "package:deb": "electron-installer-debian --src ./packages/BFS-APP-linux-x64 --dest packages/debs --icon=assets/images/bfs-app-icon-v5-512.png --arch amd64",
    "package:win": "electron-packager . bfs-dice_0.1.0_RC2 --out=packages --platform=win32 --arch=x64 --overwrite --prune=true --icon=assets/images/bfs-app-icon-v5-512.ico --ignore=dev --ignore=.gitignore --ignore=.git --ignore=gpg-pub-key --ignore=tests --ignore=releases",
    "package:mac": "electron-packager . bfs-dice_0.1.0_RC2 --overwrite --platform=darwin --arch=x64 --out=packages --prune=true --icon=assets/images/bfs-app.icns --ignore=dev --ignore=.gitignore --ignore=.git --ignore=gpg-pub-key --ignore=tests --ignore=releases"
  },
  "repository": "https://github.com/bitcoinfromscratch/bfs-dice.git",
  "keywords": [
    "bitcoin",
    "seed",
    "dice roll",
    "wallet",
    "cold storage",
    "bitcoin seed generator"
  ],
  "author": "btcfromscratch@protonmail.com",
  "license": "MIT",
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "browserify": "^16.5.0",
    "electron": "^6.0.9",
    "electron-installer-debian": "^2.0.1",
    "electron-packager": "^14.0.6",
    "jquery": "^3.4.1",
    "marked": "^0.7.0",
    "popper.js": "^1.15.0"
  },
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "bip39": "^3.0.2",
    "electron-store": "^5.0.0",
    "express": "^4.17.1",
    "path": "^0.12.7"
  }
}
